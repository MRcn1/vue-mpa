{
  "name": "qwy",
  "version": "2.1.0",
  "description": "A Vue.js project use webpack4",
  "author": "Blaze",
  "main": "./build/main.js",
  "private": true,
  "scripts": {
    "dev": "cross-env BABEL_ENV=development webpack-dev-server --inline --config build/webpack.dev.conf.js",
    "dev:analysis": "cross-env BABEL_ENV=development webpack-dev-server --inline --profile --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "dll": "webpack --config build/webpack.dll.conf.js --mode production",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "node --max_old_space_size=4096 build/build.js --build-all",
    "build:base": "node build/build.js --build-base",
    "build:crm": "node build/build.js --build-crm",
    "build:form": "node build/build.js --build-form",
    "build:alone": "node build/build.js",
    "build:test": "node build/build.js --build-buildTest",
    "build:report": "npm run build --report",
    "build:alone:report": "npm run build:alone --report",
    "build:alone:analysis": "webpack --profile --progress --config build/build.js",
    "build:all:analysis": "webpack --profile --progress --config build/build.js --build-all",
    "auto:build": "node --max_old_space_size=4096 build/auto-build.js --build-all",
    "auto:build:base": "node --max_old_space_size=4096 build/auto-build.js --build-base",
    "auto:build:debug": "node --inspect-brk --max_old_space_size=4096 build/auto-build.js",
    "build:file": "node build/build-components-list.js"
  },
  "dependencies": {
    "@handsontable/vue": "^3.0.0",
